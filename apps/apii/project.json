{
  "name": "apii",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/apii/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest build apii"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest start apii --watch"
      }
    },
    "migration": {
    "executor": "nx:run-commands",
    "options": {
      "command": "yarn mikro-orm migration:create --config apps/apii/src/mikro-orm.config.ts"
    }
  },
    "migrate:up": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn mikro-orm migration:up --config apps/apii/src/mikro-orm.config.ts"
      }
    }
  }
}
